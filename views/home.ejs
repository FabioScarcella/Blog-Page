<%- include("partials/header") -%>

<h1>Home</h1>
<p><%= homeStartingContent %></p>

<% posts.forEach(function(post){ %>
  <div class="mt-3">
    <h1><%= post.title %></h1>
    <p><%= post.text.slice(0, 100) + " ..." %>
    <a href="/posts/<%= post._id %>"> Read More </a>
    </p>
  </div>
<% }) %>



